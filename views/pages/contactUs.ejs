                        <form action="/submitquery" method="POST">
                            <h3 class="text-center" style="color: black;">CONTACT FORM</h3>
                            <div class="form-group">
                                <label for="name">Name of Society/Organization</label>
                                <input type="text" class="form-control" name="name" aria-describedby="emailHelp" placeholder="Enter name" required>
                            </div>
                            <div class="form-group">
